  <inherits name="org.slf4j.Slf4j" />
  <inherits name="javax.persistence.Persistence" />

  <inherits name="org.springframework.http.Http" />
  <inherits name="org.springframework.security.Security" />
  <inherits name="org.springframework.security.UserDetails" />
  <inherits name="org.springframework.security.oauth2.OAuth2User" />
